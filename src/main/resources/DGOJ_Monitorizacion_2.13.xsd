					<xs:element name="PremiosEspecie" type="DesgloseOperador"/>
					<xs:element name="Botes" type="DesgloseOperador"/>
					<xs:element name="AjustesRed" type="DesgloseOperador"/>
					<xs:element name="Otros" type="DesgloseOperadorConcepto">
